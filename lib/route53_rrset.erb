<?xml version="1.0" encoding="UTF-8"?>
<ChangeResourceRecordSetsRequest xmlns="https://route53.amazonaws.com/doc/2012-02-29/">
    <ChangeBatch>
        <Changes>
            <Change>
                <Action><%= action %></Action>
                <ResourceRecordSet>
                    <Name><%= name %></Name>
                    <Type><%= type %></Type>
                    <TTL><%= ttl %></TTL>
                    <ResourceRecords>
                        <ResourceRecord>
                            <Value><%= value %></Value>
                        </ResourceRecord>
                    </ResourceRecords>
                </ResourceRecordSet>
            </Change>
        </Changes>
    </ChangeBatch>
</ChangeResourceRecordSetsRequest>
